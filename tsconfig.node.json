{
  "compilerOptions": {
    "module": "node16",
    "moduleResolution": "node16",
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "types": [
      "node"
    ],
    "skipLibCheck": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true
  },
  "include": [
    "vite.config.ts"
  ]
}