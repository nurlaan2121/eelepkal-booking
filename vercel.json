{
    "buildCommand": "tsc -b && vite build",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/proxy"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}